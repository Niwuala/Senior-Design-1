#include <LiquidCrystal_I2C.h>
#include <Wire.h>2
#include <Adafruit_Sensor.h>
#include <DHT.h>
#include <DHT_U.h>
#include <ESP8266WiFi.h>
#include <ESP8266HTTPClient.h>

int DHTPIN = 14;
#define DHTTYPE DHT22



int rPin_1 = 5;
int rPin_2 = 4;
int rPin_3 = 0;

DHT dht(DHTPIN, DHTTYPE);
 
const char* ssid = "Ndubuisi's iPhone";
const char* password = "Daniel22!";
float temp;
float convert;
LiquidCrystal_I2C lcd(0x27, 16, 2);  // set the LCD address to 0x27 for a 16 chars and 2 line display

void setup () {
 pinMode(rPin_1,OUTPUT);
 pinMode(rPin_2,OUTPUT);
 pinMode(rPin_3,OUTPUT);
 Serial.begin(115200);
 dht.begin();
 Wire.begin(12,13);
  lcd.begin(16,2);
  lcd.backlight();
  WiFi.begin(ssid, password);
  
  while (WiFi.status() != WL_CONNECTED) {
 
    delay(1000);
    Serial.println("Connecting..");
 
  }
}
 
void loop() {

 
  if (WiFi.status() == WL_CONNECTED) { //Check WiFi connection status
     temp = dht.readTemperature();
     convert = temp * 1.8 + 32;
    HTTPClient http;  //Declare an object of class HTTPClient
     
    http.begin("http://api.thingspeak.com/channels/623673/fields/1/last.txt");  //Specify request destination
    int httpCode = http.GET();                                                 //Send the request
             
    if (httpCode = 200) { //Check the returning code
 
      String payload = http.getString();   //Get the request response payload
      Serial.println(payload);                     //Print the response payload

     if (payload == "60") {
      if (convert <= 60.00) {
        digitalWrite(rPin_1, LOW);
        digitalWrite(rPin_2, HIGH);
        digitalWrite(rPin_3, LOW);
        lcd.clear();
          lcd.print("CURR: TARG: MODE");
          lcd.setCursor(0,1);
          lcd.print(convert);
          lcd.setCursor(6,1);
          lcd.print("60");
          lcd.setCursor(12,1);
          lcd.print("HEAT");
      } if (convert > 60.00) {
        digitalWrite(rPin_1, HIGH);
        digitalWrite(rPin_2, HIGH);
        digitalWrite(rPin_3, HIGH);
        lcd.clear();
          lcd.print("CURR: TARG: MODE");
          lcd.setCursor(0,1);
          lcd.print(convert);
          lcd.setCursor(6,1);
          lcd.print("60");
          lcd.setCursor(12,1);
          lcd.print("HEAT");
      }
     }
        
