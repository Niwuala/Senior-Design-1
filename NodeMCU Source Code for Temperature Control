          lcd.print("A/C");
        }
        }

          
     if (payload == "183") { 
      if (convert > 83.00) {
          digitalWrite(rPin_1, HIGH);
          digitalWrite(rPin_2, LOW);
          digitalWrite(rPin_3, LOW);
          lcd.clear();
          lcd.print("CURR: TARG: MODE");
          lcd.setCursor(0,1);
          lcd.print(convert);
          lcd.setCursor(6,1);
          lcd.print("83");
          lcd.setCursor(12,1);
          lcd.print("A/C");
        } if (convert <= 83.00) {
          digitalWrite(rPin_1,HIGH);
          digitalWrite(rPin_2,HIGH);
          digitalWrite(rPin_2,HIGH);
          lcd.clear();
          lcd.print("CURR: TARG: MODE");
          lcd.setCursor(0,1);
          lcd.print(convert);
          lcd.setCursor(6,1);
          lcd.print("83");
          lcd.setCursor(12,1);
          lcd.print("A/C");
        }
        }

          
      if (payload == "184") { 
      if (convert > 84.00) {
          digitalWrite(rPin_1, HIGH);
          digitalWrite(rPin_2, LOW);
          digitalWrite(rPin_3, LOW);
          lcd.clear();
          lcd.print("CURR: TARG: MODE");
          lcd.setCursor(0,1);
          lcd.print(convert);
          lcd.setCursor(6,1);
          lcd.print("84");
          lcd.setCursor(12,1);
          lcd.print("A/C");
        } if (convert <= 84.00) {
          digitalWrite(rPin_1,HIGH);
          digitalWrite(rPin_2,HIGH);
          digitalWrite(rPin_2,HIGH);
          lcd.clear();
          lcd.print("CURR: TARG: MODE");
          lcd.setCursor(0,1);
          lcd.print(convert);
          lcd.setCursor(6,1);
          lcd.print("84");
          lcd.setCursor(12,1);
          lcd.print("A/C");
        }
        }


      if (payload == "185") { 
      if (convert > 85.00) {
          digitalWrite(rPin_1, HIGH);
          digitalWrite(rPin_2, LOW);
          digitalWrite(rPin_3, LOW);
          lcd.clear();
          lcd.print("CURR: TARG: MODE");
          lcd.setCursor(0,1);
          lcd.print(convert);
          lcd.setCursor(6,1);
          lcd.print("85");
          lcd.setCursor(12,1);
          lcd.print("A/C");
        } if (convert <= 85.00) {
          digitalWrite(rPin_1,HIGH);
          digitalWrite(rPin_2,HIGH);
          digitalWrite(rPin_2,HIGH);
          lcd.clear();
          lcd.print("CURR: TARG: MODE");
          lcd.setCursor(0,1);
          lcd.print(convert);
          lcd.setCursor(6,1);
          lcd.print("85");
          lcd.setCursor(12,1);
          lcd.print("A/C");
        }
        }
        
  http.end(); 
 
    }
delay(1000);


}

